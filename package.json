  "license": "MIT",
  "dependencies": {
    "chokidar": "^2.0.4",
    "cors": "^2.8.4",
    "dockerode": "^2.5.5",
    "fastify": "^1.8.0",
    "fastify-auth": "^0.3.0",
    "highland": "^2.13.0",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "lokijs": "^1.5.5",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.1.2",
    "rimraf": "^2.6.2",
    "semver-compare": "^1.0.0",
    "signale": "^1.2.1",
    "sshpk": "^1.14.2",
    "tar-fs": "^1.16.3",
    "uuid": "^3.3.2"
