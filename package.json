{
  "name": "exoframe-server",
  "version": "3.5.0-dev",
  "description": "Exoframe is a self-hosted tool that allows simple one-command deployments using Docker",
  "main": "exoframe-server.js",
  "bin": "exoframe-server.js",
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=testing jest --coverage --silent --maxWorkers=2 --ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "pkg -t node10.4.1-alpine -o exoframe-server .",
    "build-for-npm": "ncc build index.js -o dist"
  },
  "pkg": {
    "assets": "src/templates/*.html"
  },
  "files": [
    "exoframe-server.js"
  ],
  "repository": "git@github.com:exoframejs/exoframe-server.git",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chokidar": "^2.0.4",
    "cors": "^2.8.4",
    "dockerode": "^2.5.6",
    "fastify": "^1.14.0",
    "fastify-auth": "^0.3.0",
    "highland": "^2.13.0",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "rimraf": "^2.6.2",
    "semver-compare": "^1.0.0",
    "signale": "^1.3.0",
    "sshpk": "^1.14.2",
    "tar-fs": "^2.0.0",
    "uuid": "^3.3.2",
    "@zeit/ncc": "^0.14.0",
    "coveralls": "^3.0.2",
    "get-port": "^4.0.0",
    "jest": "^24.0.0",
    "nock": "^10.0.4",
    "pkg": "^4.3.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
