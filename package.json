{
  "name": "exoframe-server",
  "version": "1.0.1",
  "description": "Exoframe is a self-hosted tool that allows simple one-command deployments using Docker",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=testing tap --timeout=300 --cov --coverage-report=lcov --reporter=spec ./test/index.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "pkg -t node8.6.0-alpine -o exoframe-server ."
  },
  "pkg": {
    "assets": "src/templates/*.html"
  },
  "repository": "git@github.com:exoframejs/exoframe-server.git",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^1.7.0",
    "dockerode": "^2.4.3",
    "hapi": "^17.0.0",
    "hapi-auth-jwt": "^4.0.0",
    "highland": "^2.11.1",
    "js-yaml": "^3.9.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lokijs": "^1.5.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.3",
    "rimraf": "^2.6.1",
    "semver-compare": "^1.0.0",
    "sshpk": "^1.13.1",
    "tar-fs": "^1.15.2",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "pkg": "^4.1.4",
    "tap": "^10.7.1"
  }
}
